{
  "name": "react-redux-webpack-boilerplate",
  "sideEffects": [
    "*.scss"
  ],
  "version": "1.0.0",
  "description": "A boilerplate for creating web applications using React, Redux, React-Router and Webpack",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot --display-error-details",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "test": "jest ./src/test"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/test/enzyme.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eddcp/react-redux-router-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router"
  ],
  "author": "Eduardo Paiva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Eddcp/react-redux-router-boilerplate/issues"
  },
  "homepage": "https://github.com/Eddcp/react-redux-router-boilerplate#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-test-renderer": "^16.8.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
